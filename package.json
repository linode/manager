    "chart.js": "^2.5.0",
    "circular-dependency-plugin": "^2.0.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.6",
    "css-loader": "0.28.7",
    "d3": "^3.5.17",
    "deep-freeze": "0.0.1",
    "dotenv": "4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "1.9.0",
