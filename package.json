{
  "name": "linode-manager",
  "version": "0.16.2",
  "description": "The Linode Manager website",
  "scripts": {
    "clean": "rm -rf dist",
    "copy-assets": "./bin/copy-assets.sh",
    "build:webpack": "yarn run copy-assets && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "yarn run clean && yarn run build:webpack",
    "kill": "lsof -i :3000 | grep node | tr -s ' ' | cut -d ' ' -f2 | xargs kill",
    "lint": "eslint --max-warnings=0 src test",
    "lint:fix": "eslint src test --fix",
    "start": "yarn run copy-assets && node devServer.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Linode/manager.git"
  },
  "keywords": [],
  "author": "Linode",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Linode/manager/issues"
  },
  "homepage": "https://github.com/Linode/manager",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-root-import": "^4.1.0",
    "babylon": "^6.16.1",
    "circular-dependency-plugin": "^2.0.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.9.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jsonpath": "^0.2.11",
    "mockdate": "^1.0.4",
    "node-sass": "^3.4.2",
    "pre-commit": "^1.2.2",
    "qrious": "^2.2.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.1.2",
    "sinon": "^4.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.4",
    "xunit-file": "^0.0.9"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "chart.js": "^2.5.0",
    "d3": "^3.5.17",
    "deep-freeze": "0.0.1",
    "flag-icon-css": "^2.3.0",
    "font-awesome": "^4.7.0",
    "fontawesome": "^4.5.0",
    "history": "^2.1.2",
    "isomorphic-fetch": "^2.2.0",
    "jest": "^21.2.1",
    "linode-components": "file:./components",
    "linode-styleguide": "file:./styleguide",
    "lodash": "^4.16.4",
    "md5": "^2.1.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "qrious": "^2.2.0",
    "raven-js": "^3.16.0",
    "rd3": "^0.6.3",
    "react": "^15.6",
    "react-ace": "^5.1.1",
    "react-dom": "^15.6",
    "react-guard": "^0.4.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "react-side-effect": "^1.1.3",
    "react-tabs": "^0.7.0",
    "react-test-renderer": "15.5",
    "react-tooltip": "^3.3.0",
    "redux": "^3.0.5",
    "redux-thunk": "^2.1.0",
    "xterm": "^2.0.1",
    "zxcvbn": "^4.4.2"
  },
  "pre-commit": ["test", "lint"],
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/testSetup.js",
    "testRegex": "src/.*\\.spec\\.js$",
    "transformIgnorePatterns": [
      "/node_modules/(?!(linode-components-*.*?\\.js$))"
    ],
    "globals": {
      "ENV_API_ROOT": null,
      "ENV_LOGIN_ROOT": null,
      "ENV_APP_ROOT": null,
      "ENV_GA_ID": null,
      "ENV_SENTRY_URL": null,
      "ENV_DEVTOOLS_DISABLED": null,
      "ENV_VERSION": null
    }
  }
}
