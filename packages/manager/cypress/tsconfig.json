{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "target": "es5",
    "lib": ["es5", "dom"],
    "baseUrl": "..",
    "paths": {
      "@src/*": ["src/*"]
    },
    "types": ["cypress", "cypress-file-upload"]
  },
  "include": ["../cypress/**/*.ts", "**/*.*"]
}