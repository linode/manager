{
    "extends": ["tslint-config-airbnb"],
    "rules": {
        "no-console": {
            "severity": "warning"
        },
        "align": {
          "severity": "warning"
        },
        "no-trailing-whitespace": true,
        "variable-name": false,
        "import-name": false,
        "object-shorthand-properties-first": false,
        "strict-boolean-expressions": false
    }
}
