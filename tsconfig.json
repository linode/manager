{
  "exclude": [
    "coverage",
    "node_modules",
    "dist",
    "stats.json",
    "assets"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
        "~/*": [ "./src/*" ]
    }
  }
}